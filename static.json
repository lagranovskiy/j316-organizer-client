{
  "root": "dist/",
  "clean_urls": false,
  "proxies": {
    "/api/": {
      "origin": "https://j316-organizer-backend.herokuapp.com/"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=512000"
    },
    "/assets/webfonts/*": {
      "Access-Control-Allow-Origin": "*"
    }
  },
  "routes": {
    "/**": "index.html"
  }
}
